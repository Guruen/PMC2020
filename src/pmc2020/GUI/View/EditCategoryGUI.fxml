<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="140.0" prefWidth="195.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pmc2020.GUI.EditCategoryGUIController">
   <children>
      <Button fx:id="editButton" layoutX="93.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleEditCategoryButton" text="Edit Category" />
      <TextField fx:id="editCategoryTextFieldText" layoutX="19.0" layoutY="57.0" promptText="New category title" />
      <ComboBox fx:id="CatChooserToEdit" layoutX="18.0" layoutY="14.0" onAction="#handleCategoryComboBox" prefWidth="150.0" promptText="Choose Category" />
      <Button layoutX="19.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleDelete" text="Delete" />
   </children>
</AnchorPane>
